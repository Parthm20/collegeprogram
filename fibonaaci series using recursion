#include<stdio.h>
#include<conio.h>
int fibonacci(int n){ 
	if(n<=1) return n; 
	 
	else
	return (fibonacci(n-1)+fibonacci(n-2));
} 
int main(){ 
	int n; 
	printf("Enter the number of element :\n"); 
	scanf("%d",&n); 
	printf("fibonacci series is : \n");
	for(int i=0;i<n;i++) { 
		printf("%d ",fibonacci(i));
	}
//	getch();
}
 
